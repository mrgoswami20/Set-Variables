pragma solidity >=0.8.9;

contract variableTypes {

   uint number;
   int a;
   string bol;
   address d;

    function setFunction(uint _number, int _a, string memory _bol, address _d) public {
          number = _number;
          a = _a;
          bol = _bol;
          d = _d;
    }

    // returns the value of the state variable number
    function getFunction() public view returns (uint) {
        return number;
    }
 }
